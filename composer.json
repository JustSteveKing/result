{
    "name": "juststeveking/result",
    "description": "A tiny, framework-agnostic Result type for PHP 8.4 that makes error handling explicit, composable, and testable.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "result",
        "error-handling",
        "functional",
        "monad",
        "ok",
        "err",
        "laravel"
    ],
    "homepage": "https://github.com/JustSteveKing/result",
    "support": {
        "issues": "https://github.com/JustSteveKing/result/issues",
        "source": "https://github.com/JustSteveKing/result"
    },
    "authors": [
        {
            "role": "Maintainer",
            "name": "Steve McDougall",
            "email": "juststeveking@proton.me",
            "homepage": "https://juststeveking.link"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3.15",
        "laravel/pint": "^1.25.1",
        "phpstan/phpstan": "^2.1.29",
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan-strict-rules": "^2.0.7"
    },
    "autoload": {
        "psr-4": {
            "JustSteveKing\\Result\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JustSteveKing\\Result\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox",
        "stan": "./vendor/bin/phpstan analyse --memory-limit=-1",
        "pint": "./vendor/bin/pint",
        "lint": [
            "@stan",
            "@pint"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
